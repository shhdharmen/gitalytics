{
  "name": "gitalytics",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "verify:client": "ts-node ./scripts/verify-frontend-build.ts",
    "verify:api": "ts-node ./scripts/verify-backend-build.ts",
    "config": "ts-node ./scripts/set-env.ts",
    "commit": "cz",
    "ng": "nx",
    "nx:script": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "npm run config && ng serve",
    "client:build": "nx build client --prod",
    "server:build": "nx build api --prod",
    "build": "ng build",
    "build:prod:client": "run-s config nx:script client:build",
    "build:prod:server": "run-s nx:script server:build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "deploy:start:client": "serve -s dist/apps/client -l tcp://0.0.0.0:$PORT -n",
    "deploy:start:server": "node dist/apps/api/main.js",
    "generate-codegen": "graphql-codegen --config codegen.yml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@apollo/client": "^3.3.6",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.9.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nrwl/angular": "11.0.18",
    "apollo-angular": "^2.1.0",
    "apollo-server-express": "^2.19.0",
    "bootstrap": "^4.5.3",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "html2canvas": "^1.0.0-rc.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "serve": "^11.3.2",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-apollo-angular": "2.2.1",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "11.0.18",
    "@nrwl/cypress": "11.0.18",
    "@nrwl/eslint-plugin-nx": "11.0.18",
    "@nrwl/jest": "11.0.18",
    "@nrwl/nest": "11.0.18",
    "@nrwl/node": "11.0.18",
    "@nrwl/nx-cloud": "10.1.10",
    "@nrwl/tao": "11.0.18",
    "@nrwl/workspace": "^11.0.18",
    "@types/hapi__joi": "^17.1.6",
    "@types/html2canvas": "0.0.36",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@types/npm-run": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "cypress": "^5.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "husky": "^4.3.6",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "^10.5.3",
    "npm-run": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "~4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "nx lint --fix && git add . && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.scss": "stylelint --fix --syntax=scss",
    "*.{js,json,md,ts,html}": [
      "prettier --write"
    ]
  }
}
